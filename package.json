{
  "name": "@vechr/vechr-base",
  "version": "0.0.1",
  "description": "Vechr Base Package - A NestJS-based foundation for building scalable applications",
  "author": "Vechr Team",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "branches": [
      "master"
    ]
  },
  "license": "MIT",
  "scripts": {
    "commit": "cz",
    "build": "rm -rf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "watch": "nest start --debug 0.0.0.0:9229 --watch",
    "dev": "nest start",
    "db:studio": "prisma studio --schema './database/schema.prisma'",
    "db:migrations": "prisma migrate dev --schema './database/schema.prisma' --create-only --name",
    "db:migrate": "prisma migrate dev --schema './database/schema.prisma'",
    "prisma:sync": "prisma generate --schema './database/schema.prisma'",
    "prepare": "husky",
    "prisma:prod:deploy": "prisma migrate deploy --schema './database/schema.prisma'",
    "prestart:prod": "npm run prisma:prod:deploy",
    "start:prod": "node dist/server",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@keyv/redis": "^4.3.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/context-async-hooks": "^2.0.0",
    "@opentelemetry/exporter-prometheus": "^0.200.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/instrumentation-express": "^0.48.1",
    "@opentelemetry/instrumentation-nestjs-core": "^0.46.0",
    "@opentelemetry/instrumentation-winston": "^0.45.0",
    "@opentelemetry/propagator-b3": "^2.0.0",
    "@opentelemetry/propagator-jaeger": "^2.0.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@prisma/client": "^6.6.0",
    "@prisma/instrumentation": "^6.6.0",
    "@redis/json": "^1.0.7",
    "cache-manager": "^6.4.2",
    "cacheable": "^1.8.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "env-var": "^7.5.0",
    "express-winston": "^4.2.0",
    "glob": "^11.0.2",
    "jsonwebtoken": "^9.0.2",
    "md5": "^2.3.0",
    "ms": "^2.1.3",
    "nats": "^2.29.3",
    "nest-winston": "^1.10.2",
    "nestjs-otel": "^6.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "randomstring": "^1.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "short-uuid": "^5.2.0",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/crypto-js": "^4.2.2",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "22.15.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "semantic-release": "^24.2.3",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
